{
  "compilerOptions": {
      "noImplicitAny": false,
      "noEmitOnError": true,
      "removeComments": false,
      "sourceMap": true,
      "target": "ES5",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "module": "es2015",
      "lib": ["ES5", "dom", "es2015"],
      "outDir": "dist",
      "allowSyntheticDefaultImports": true
  },
  "include": [
      "src/js/**/*"
  ]
}